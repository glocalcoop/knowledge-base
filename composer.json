    "type": "wordpress-plugin",
    "require-dev": {
        "phpunit/phpunit": "^5.2"
    },
    "license": "GPL-3",
    "authors": [
        {
            "name": "Pea Lutz",
            "email": "pea@glocal.coop"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "meitar/wp-screen-help-loader",
                "version": "master",
